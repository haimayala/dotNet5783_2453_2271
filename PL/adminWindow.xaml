        </Button>
        <Button x:Name="btnProducts" Style="{StaticResource buttonStyle}" Content="Order list" HorizontalAlignment="Left" Margin="84,86,0,0" VerticalAlignment="Top" Height="41" Width="134" Click="btnProducts_Click"/>

    </Grid>
</Window>
