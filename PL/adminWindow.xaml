<Window x:Class="PL.adminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="adminWindow" Height="450" Width="800">


    <Grid  >
        <Button x:Name="btnOrders" Style="{StaticResource buttonStyle}" Content="Product list" HorizontalAlignment="Left" Margin="472,313,0,0" VerticalAlignment="Top" Height="63" Width="186" Click="btnOrders_Click">

        </Button>
        <Button x:Name="btnProducts" Style="{StaticResource buttonStyle}" Content="Order list" HorizontalAlignment="Left" Margin="166,320,0,0" VerticalAlignment="Top" Height="56" Width="196" Click="btnProducts_Click"/>
        <Label Content="Mannager" HorizontalAlignment="Left" Margin="570,121,0,0" VerticalAlignment="Top" Width="339" Height="109" FontSize="72" FontFamily="Blackadder ITC" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="90.256"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>
