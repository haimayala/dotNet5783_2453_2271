        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="adminWindow" Height="450" Width="800">


    <Grid  Style="{StaticResource  gridSyle}">
